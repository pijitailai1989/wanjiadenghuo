.item{
  justify-content: space-around;
  padding: 69rpx 25rpx;
  flex-flow: row wrap;
}
.item>view{
  text-align: center;
  width: 314rpx;
  height: 280rpx;
  font-size: 38rpx;
  color: #fff;
  line-height: 1;
  border-radius: 10rpx;
  position: relative;
  margin-bottom: 30rpx;
}

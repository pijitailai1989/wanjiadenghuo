.item>view image{
  width: 100%;
  height: 100%;
}
.item>view view{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 70rpx;
}
.item>view navigator{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}

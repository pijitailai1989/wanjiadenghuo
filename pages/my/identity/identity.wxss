/* pages/my/myprofit/identity/identity.wxss */
@import '../model/model.wxss';
.inputbox{
  background: #fff;
  padding: 58rpx 0;
  margin: 15rpx 20rpx;
  border-radius: 10rpx;
}
.li{
  height: 68rpx;
  padding-right: 80rpx;
  padding-left: 50rpx;
  margin-bottom: 30rpx;
}
.li .label{
  width: 160rpx;
  height: 100%;
  line-height: 68rpx;
}
.li .flex1{
  background: #f1f1f1;
  border-radius: 10rpx;
  line-height: 68rpx;
  height: 100%;
  white-space: nowrap;
  text-overflow: ellipsis;
}

/*图片列表  */
/* 网红列表  */

.item_list {
  margin: 0 24rpx;
}

.item_list .tips {
  font-size: 26rpx;
  padding: 0 20rpx;
  line-height: 60rpx;
}

.item_list .input {
  background: #fff;
  height: 72rpx;
  border-radius: 10rpx;
  display: flex;
}

.item_list .input input {
  display: block;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding: 0 20rpx;
}

.personal.item_list .input {
  height: 138rpx;
  align-items: flex-start;
}

.personal.item_list .input textarea {
  display: block;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding: 10rpx 20rpx;
}
/* 上传图片  */
.weui-cells{
  margin: 0;
}
.item_list .content{
  padding: 20rpx 20rpx 0;
  background: #fff;
}
.item_list  .title{
  padding: 0 20rpx;
}
.weui-uploader__input-box {
  border-style: dashed;
}
.submit{
  height: 90rpx;
  line-height: 90rpx;
  margin:20rpx 40rpx;
}
.personal_pic .content{
  padding: 20rpx 0 20rpx 10rpx;
}
